{% extends 'base.html.twig' %}

{% block title %}List of Users{% endblock %}

{% block body %}

{% if app.user %}
    <div class="mb-3">
        You are logged in as {{ app.user.username }}, <a href="{{ path('app_logout') }}">Logout</a>
    </div>
{% endif %}

{% for user in users %}
    <li>{{ user.username|e }}</li>
{% endfor %}

{% endblock %}
